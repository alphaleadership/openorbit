#version 150



uniform mat4 oo_ModelViewMatrix;
uniform mat4 oo_ProjectionMatrix;

in vec4 oo_Vertex0;
in vec2 oo_TexCoord0;
in vec4 oo_Color0;

out vec2 FragTexCoord;
out vec4 FragColor;

void
main()
{
  FragTexCoord = oo_TexCoord0;
  FragColor = oo_Color0;
  gl_Position = oo_ProjectionMatrix * oo_ModelViewMatrix * oo_Vertex0;
}
